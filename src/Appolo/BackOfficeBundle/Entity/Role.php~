<?php

namespace Appolo\BackOfficeBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Role
 */
class Role
{
    /**
     * @var string
     */
    private $libellerole;

    /**
     * @var integer
     */
    private $idrole;

    /**
     * @var \Doctrine\Common\Collections\Collection
     */
    private $iduser;

    /**
     * Constructor
     */
    public function __construct()
    {
        $this->iduser = new \Doctrine\Common\Collections\ArrayCollection();
    }

    /**
     * Set libellerole
     *
     * @param string $libellerole
     * @return Role
     */
    public function setLibellerole($libellerole)
    {
        $this->libellerole = $libellerole;

        return $this;
    }

    /**
     * Get libellerole
     *
     * @return string 
     */
    public function getLibellerole()
    {
        return $this->libellerole;
    }

    /**
     * Get idrole
     *
     * @return integer 
     */
    public function getIdrole()
    {
        return $this->idrole;
    }

    /**
     * Add iduser
     *
     * @param \Appolo\BackOfficeBundle\Entity\User $iduser
     * @return Role
     */
    public function addIduser(\Appolo\BackOfficeBundle\Entity\User $iduser)
    {
        $this->iduser[] = $iduser;

        return $this;
    }

    /**
     * Remove iduser
     *
     * @param \Appolo\BackOfficeBundle\Entity\User $iduser
     */
    public function removeIduser(\Appolo\BackOfficeBundle\Entity\User $iduser)
    {
        $this->iduser->removeElement($iduser);
    }

    /**
     * Get iduser
     *
     * @return \Doctrine\Common\Collections\Collection 
     */
    public function getIduser()
    {
        return $this->iduser;
    }
}
