<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Appolo\BackOfficeBundle\Entity\Roleuser" table="roleuser">
    <id name="idroleus" type="integer" column="idRoleUs">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleroleus" type="text" column="libelleRoleUs" nullable="false"/>
    <many-to-many field="iduser" target-entity="User" mapped-by="idroleus"/>
  </entity>
</doctrine-mapping>
