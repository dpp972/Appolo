<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Appolo\BackOfficeBundle\Entity\Adresse" table="adresse">
    <indexes>
      <index name="FK_Adresse_idUtilisateur" columns="idUtilisateur"/>
      <index name="FK_Adresse_idType" columns="idType"/>
    </indexes>
    <id name="idadresse" type="integer" column="idAdresse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomvoie" type="text" column="nomVoie" nullable="true"/>
    <field name="numerovoie" type="text" column="numeroVoie" nullable="true"/>
    <many-to-one field="idtype" target-entity="Typevoie">
      <join-columns>
        <join-column name="idType" referenced-column-name="idType"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idutilisateur" target-entity="Utilisateur">
      <join-columns>
        <join-column name="idUtilisateur" referenced-column-name="idUtilisateur"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idcommande" target-entity="Utilisateur" inversed-by="idadresse">
      <join-table name="livrercommande">
        <join-columns>
          <join-column name="idAdresse" referenced-column-name="idAdresse"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="idCommande" referenced-column-name="idCommande"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
