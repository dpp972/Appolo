<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Appolo\BackOfficeBundle\Entity\Panier" table="panier">
    <indexes>
      <index name="FK_Panier_idUser" columns="idUser"/>
    </indexes>
    <id name="idpanier" type="integer" column="idPanier">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ipuserpanier" type="text" column="ipUserPanier" nullable="true"/>
    <field name="numsesspanier" type="text" column="numSessPanier" nullable="true"/>
    <many-to-one field="iduser" target-entity="User">
      <join-columns>
        <join-column name="idUser" referenced-column-name="idUser"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idproduit" target-entity="Produit" inversed-by="idpanier">
      <join-table name="contenirproduit">
        <join-columns>
          <join-column name="idPanier" referenced-column-name="idPanier"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="idProduit" referenced-column-name="idProduit"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
