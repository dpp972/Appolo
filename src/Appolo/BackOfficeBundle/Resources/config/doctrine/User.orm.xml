<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Appolo\BackOfficeBundle\Entity\User" table="user">
    <indexes>
      <index name="FK_User_idAdresseLiv" columns="idAdresseLiv"/>
      <index name="FK_User_idPanier" columns="idPanier"/>
    </indexes>
    <id name="iduser" type="integer" column="idUser">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomuser" type="text" column="nomUser" nullable="true"/>
    <field name="prenomuser" type="text" column="prenomUser" nullable="true"/>
    <field name="identifiantuser" type="text" column="identifiantUser" nullable="true"/>
    <field name="datenaissuser" type="date" column="dateNaissUser" nullable="true"/>
    <field name="passworduser" type="text" column="passwordUser" nullable="true"/>
    <field name="saltuser" type="text" column="saltUser" nullable="true"/>
    <field name="emailuser" type="text" column="emailUser" nullable="true"/>
    <many-to-one field="idpanier" target-entity="Panier">
      <join-columns>
        <join-column name="idPanier" referenced-column-name="idPanier"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idadresseliv" target-entity="Adresse">
      <join-columns>
        <join-column name="idAdresseLiv" referenced-column-name="idAdresse"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idcmd" target-entity="Commande" inversed-by="iduser">
      <join-table name="passercommande">
        <join-columns>
          <join-column name="idUser" referenced-column-name="idUser"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="idCmd" referenced-column-name="idCmd"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="idroleus" target-entity="Roleuser" inversed-by="iduser">
      <join-table name="avoirrole">
        <join-columns>
          <join-column name="idUser" referenced-column-name="idUser"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="idRoleUs" referenced-column-name="idRoleUs"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
