<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Appolo\BackOfficeBundle\Entity\Commande" table="commande">
    <indexes>
      <index name="FK_Commande_idAdresse" columns="idAdresse"/>
      <index name="FK_Commande_idPanier" columns="idPanier"/>
    </indexes>
    <id name="idcmd" type="integer" column="idCmd">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datecmd" type="date" column="dateCmd" nullable="false"/>
    <many-to-one field="idpanier" target-entity="Panier">
      <join-columns>
        <join-column name="idPanier" referenced-column-name="idPanier"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idadresse" target-entity="Adresse">
      <join-columns>
        <join-column name="idAdresse" referenced-column-name="idAdresse"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="iduser" target-entity="User" mapped-by="idcmd"/>
  </entity>
</doctrine-mapping>
