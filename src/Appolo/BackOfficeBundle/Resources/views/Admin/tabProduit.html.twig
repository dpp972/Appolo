<table class="table table-striped table-bordered table-condensed">
        
    <thead>
        <tr>
            <th><input type="checkbox" name="" /></th>
            <th>id</th>
            <th>Reference</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
         {% for produit in items %} 
            <tr>
                <td><input type="checkbox" name="" /></td>
                <td>{{produit.idProduit}}</td>
                <td>{{produit.refProduit}}</td>
                <td><a href="{{ path('appolo_back_office_get', {'type': 'produit','id' : produit.idProduit }) }}">editer</a> |
                    <a href="{{ path('appolo_back_office_delete', {'type': 'produit','id' : produit.idProduit }) }}">supprimer</a></td>
            </tr>
        {% endfor %}
    </tbody>
</table>
</div>
