<table class="table table-striped table-bordered table-condensed">
        
    <thead>
        <tr>
            <th><input type="checkbox" name="" /></th>
            <th>ID</th>
            <th>Nom</th>
            <th>Prenom</th>
            <th>Identifiant</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
       {% for user in items %} 
           
           <tr>
                <td><input type="checkbox" name="" /></td>
                <td>{{user.iduser}}</td>
                <td>{{user.nomuser}}</td>
                <td>{{user.prenomuser}}</td>
                <td>{{user.identifiantuser}}</td>
                 <td>{{user.emailuser}}</td>
                <td><a href="{{ path('appolo_back_office_get', {'type': 'user','id' : user.iduser }) }}">editer</a> |
                    <a href="{{ path('appolo_back_office_delete', {'type': 'user','id' : user.iduser }) }}">supprimer</a></td>
            </tr>
        {% endfor %}
    </tbody>
</table>
</div>
